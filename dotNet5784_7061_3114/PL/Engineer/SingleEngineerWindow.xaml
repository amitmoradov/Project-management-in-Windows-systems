<Window x:Class="PL.Engineer.SingleEngineerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL.Engineer"
        mc:Ignorable="d"
        Title="SingleEngineerWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <StackPanel>
                <Label Content="ID:"/>
                <Label Content="Name:"/>
                <Label Content="Email: "/>
                <Label Content="Level"/>
                <Label Content="Cost"/>
                <Label Content="Can To Remove: " />
                <Label Content="Active: " />
            </StackPanel>
            <StackPanel>
                <TextBox  Margin="3" Width="100"/>
                <TextBox  Margin="3" Width="100"/>
                <TextBox  Margin="4" Width="100"/>
                <ComboBox Margin="4" Width="100"/>
                <TextBox  Margin="4" Width="100"/>
                <TextBox Margin="5" Width="100"/>
                <TextBox Margin="3" Width="100"/>
             
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
