<Window x:Class="PL.Dependency.SingelDependencyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL.Dependency"
        mc:Ignorable="d"
        Title="SingelDependencyWindow" Height="450" Width="800">
    <Grid>
        <ComboBox Grid.Column="0" SelectedValue="{Binding Path=StatusTask, Mode=TwoWay}"
          ItemsSource="{Binding Source={StaticResource StatusCollectiontKey}}" SelectionChanged="ComboBox_SelectionChanged"/>
        <ComboBox Grid.Column="1" SelectedValue="{Binding Path=StatusTask, Mode=TwoWay}"
         ItemsSource="{Binding Source={StaticResource StatusCollectiontKey}}" SelectionChanged="ComboBox_SelectionChanged"/>


    </Grid>


</Window>